{
  "Settings": {
    "LogLevel": "INFO",
    "LogFolder": "D:\\#_PROJECTS\\##_LOGS\\BYK_example_log",
    "LogFile": "CommonService.log",
    "UpdateDelay": 500
  },
  "Events": [
    {
      "Label": "ServiceInitiated",
      "ForegroundColor": "Green",
      "Enabled": true,
      "Conditions": [
        {
          "Label": "1",
          "Config": {
            "Mode": "Simple",
            "Value": "Calling Start() of Service"
          }
        }
      ]
    },
    {
      "Label": "ServiceStarted",
      "ForegroundColor": "Green",
      "Enabled": true,
      "Conditions": [
        {
          "Label": "1",
          "Config": {
            "Mode": "Simple",
            "Value": "Startup of Service done"
          }
        }
      ]
    },
    {
      "Label": "WebserverStarted",
      "ForegroundColor": "Green",
      "Enabled": true,
      "Conditions": [
        {
          "Label": "1",
          "Config": {
            "Mode": "Simple",
            "Value": "Start Webserver finished"
          }
        }
      ]
    },
    {
      "Label": "ServiceStopped",
      "ForegroundColor": "Red",
      "Enabled": true,
      "Conditions": [
        {
          "Label": "1",
          "Config": {
            "Mode": "Simple",
            "Value": "Service stopped"
          }
        }
      ]
    },
    {
      "Label": "LoadedFactionUnit_team1",
      "ForegroundColor": "Red",
      "Enabled": true,
      "Conditions": [
        {
          "Label": "1",
          "Config": {
            "Mode": "Regex",
            "Value": ".*(Success to load FactionSetup).*(team 1).*"
          }
        }
      ]
    },
    {
      "Label": "LoadedFactionUnit_team2",
      "Enabled": true,
      "ForegroundColor": "Red",
      "Conditions": [
        {
          "Label": "1",
          "Config": {
            "Mode": "Regex",
            "Value": ".*(Success to load FactionSetup).*(team 2).*"
          }
        }
      ]
    },
    {
      "Label": "UpdateMap",
      "ForegroundColor": "Magenta",
      "Enabled": true,
      "Ordered": false,
      "Conditions": [
        {
          "Label": "1",
          "Config": {
            "Mode": "Event",
            "Value": "LoadedFactionUnit_team1"
          }
        },
        {
          "Label": "2",
          "Config": {
            "Mode": "Event",
            "Value": "LoadedFactionUnit_team2"
          }
        },
        {
          "Label": "3",
          "Config": {
            "Mode": "Regex",
            "Value": ".*(LogSquad).*(Success to Determine Startup Level).*"
          }
        }
      ]
    }
  ]
}
